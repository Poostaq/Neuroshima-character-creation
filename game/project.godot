; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Tooltip",
"language": "GDScript",
"path": "res://Tooltip.gd"
} ]
_global_script_class_icons={
"Tooltip": ""
}

[application]

config/name="Neuroshima Character Creation"
run/main_scene="res://Scenes/MainScreen/MainScreen.tscn"
config/icon="res://icon.png"

[audio]

default_bus_layout=""

[autoload]

DatabaseOperations="*res://Scripts/DataStore/DatabaseOperations.gd"
GlobalConstants="*res://Scripts/GlobalConstants.gd"
Dices="*res://Scripts/Dices.gd"
CharacterStats="*res://Scripts/CharacterSheet/CharacterStats.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-sqlite/plugin.cfg" )

[locale]

test="en"
translations=PoolStringArray( "res://Translation/Ethnicity/ethnicity_description.en.translation", "res://Translation/Ethnicity/ethnicity_description.pl.translation", "res://Translation/Ethnicity/ethnicity_trait_long_description.en.translation", "res://Translation/Ethnicity/ethnicity_trait_long_description.pl.translation", "res://Translation/Ethnicity/ethnicity_trait_names.en.translation", "res://Translation/Ethnicity/ethnicity_trait_names.pl.translation", "res://Translation/Ethnicity/ethnicity_name.en.translation", "res://Translation/Ethnicity/ethnicity_name.pl.translation", "res://Translation/general_strings.en.translation", "res://Translation/general_strings.pl.translation", "res://Translation/Profession/profession_description.en.translation", "res://Translation/Profession/profession_description.pl.translation", "res://Translation/Profession/profession_name.en.translation", "res://Translation/Profession/profession_name.pl.translation", "res://Translation/Profession/profession_quote.en.translation", "res://Translation/Profession/profession_quote.pl.translation", "res://Translation/Profession/profession_trait_long_description.en.translation", "res://Translation/Profession/profession_trait_long_description.pl.translation", "res://Translation/Profession/profession_trait_names.en.translation", "res://Translation/Profession/profession_trait_names.pl.translation", "res://Translation/Profession/profession_trait_short_description.en.translation", "res://Translation/Profession/profession_trait_short_description.pl.translation", "res://Translation/Attributes/attributes_translations.en.translation", "res://Translation/Attributes/attributes_translations.pl.translation" )
locale_filter=[ 1, [ "en", "pl" ] ]

[logging]

file_logging/max_log_files=8

[physics]

common/enable_pause_aware_picking=true
