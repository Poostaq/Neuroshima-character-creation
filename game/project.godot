; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Tooltip",
"language": "GDScript",
"path": "res://Tooltip.gd"
} ]
_global_script_class_icons={
"Tooltip": ""
}

[application]

config/name="Neuroshima Character Creation"
run/main_scene="res://MainScreen.tscn"
config/icon="res://icon.png"

[audio]

default_bus_layout=""

[autoload]

DatabaseOperations="*res://src/DataStore/databaseOperations.gd"
GlobalConstants="*res://GlobalConstants.gd"
Dices="*res://Dices.gd"
CharacterStats="*res://src/CharacterSheet/CharacterStats.gd"

[display]

window/size/width=1920
window/size/height=1080
window/stretch/mode="2d"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot-sqlite/plugin.cfg" )

[logging]

file_logging/max_log_files=8

[physics]

common/enable_pause_aware_picking=true
